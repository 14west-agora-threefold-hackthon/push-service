{
  "name": "push-service",
  "version": "1.0.1",
  "description": "Microservice application",
  "main": "app.js",
  "scripts": {
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "start": "node app.js",
    "startdev": "nodemon app.js",
    "test": "mocha ./tests/**/*.spec.js",
    "api_test": "mocha ./tests/**/*api.*.test.js",
    "pretest": "eslint --ignore-path .gitignore .",
    "coverage": "nyc --reporter html --reporter text npm test"
  },
  "engines": {
    "node": ">=10.16.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/14west-agora-threefold-hackthon/push-service.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": [
        "lint-staged"
      ],
      "pre-push": "npm test",
      "commit-msg": "./node_modules/.bin/commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run --silent lint:fix",
      "git add"
    ]
  },
  "nyc": {
    "all": true,
    "check-coverage": true,
    "temp-directory": "/tmp/.nyc_output",
    "exclude": [
      "**/tests/**",
      "**/coverage/**"
    ],
    "excludeNodeModules": true,
    "branches": 80,
    "lines": 80,
    "functions": 80,
    "statements": 80
  },
  "keywords": [
    "push"
  ],
  "author": "Barry Quigley <bquigley@tfd.ie>",
  "license": "MIT",
  "dependencies": {
    "@hapi/joi": "^15.1.0",
    "async-retry": "^1.2.3",
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "camelcase": "^5.3.1",
    "chai-as-promised": "^7.1.1",
    "config-chain": "^1.1.12",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-content-length-validator": "^1.0.0",
    "express-promise-router": "^3.0.3",
    "figlet": "^1.2.3",
    "helmet": "^3.21.3",
    "hpp": "^0.2.2",
    "http-errors": "^1.7.3",
    "http-status-codes": "^1.3.2",
    "in-app-purchase": "^1.11.4",
    "joi-phone-number": "^3.0.2",
    "js2xmlparser": "^4.0.0",
    "jsonfile": "^5.0.0",
    "memoizee": "^0.4.14",
    "mjml": "^4.6.2",
    "mongodb-memory-server": "^5.2.0",
    "mongoose": "^5.6.1",
    "mustache": "^4.0.0",
    "node-fetch": "^2.6.1",
    "nodemon": "^1.19.1",
    "objection": "^1.6.9",
    "optimist": "^0.6.1",
    "parse-mongo-url": "^1.1.1",
    "postmark": "^2.5.0",
    "randomstring": "^1.1.5",
    "read-xml": "^3.0.0",
    "replay": "^2.4.0",
    "request-promise": "^4.2.4",
    "sinon": "^7.3.2",
    "sinon-express-mock": "^2.2.1",
    "swagger-jsdoc": "^3.2.9",
    "swagger-ui-express": "^4.0.6",
    "uuid": "^3.3.2",
    "validator": "^11.1.0",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.0.0",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-mocha": "^6.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^3.0.0",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.6.3",
    "lint-staged": "^9.0.2",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2"
  },
  "bugs": {
    "url": "https://github.com/14west-agora-threefold-hackthon/push-service/issues"
  },
  "homepage": "https://github.com/14west-agora-threefold-hackthon/push-service#readme",
  "directories": {
    "test": "tests"
  }
}
